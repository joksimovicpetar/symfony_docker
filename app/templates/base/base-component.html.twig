<div class="card card-base">
    <img src="{{ asset(base.imageId.imagePath) }}" alt="{{ base.imageId.imagePath }}" class="card-img-top" draggable="false">
    <div class="card-body">
        <h2 class="card-title">{{ base.name }}</h2>
        <p id="card-text">
            {{ base.description }}
        </p>
    </div>
</div>
