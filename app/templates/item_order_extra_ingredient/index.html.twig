{% extends 'base.html.twig' %}

{% block title %}Hello ItemOrderExtraIngredientController!{% endblock %}

{% block body %}
    <table id="item_order_extra_ingredient" class="table table-striped table-field">
        <thead>
        <tr>
            <th >id</th>
            <th >item_order_id</th>
            <th >ingredient_id</th>
        </tr>
        </thead>
        <tbody>
        {% for itemOrderExtraIngredient in itemOrderExtraIngredients %}

            <tr>
                <td >{{ itemOrderExtraIngredient.id }}</td>
                <td >{{ itemOrderExtraIngredient.itemOrder.id }}</td>
                <td >{{ itemOrderExtraIngredient.extraIngredient.id }}</td>

            </tr>
        {% endfor %}

        </tbody>
    </table>
{% endblock %}
