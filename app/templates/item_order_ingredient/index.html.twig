{% extends 'base.html.twig' %}

{% block title %}Hello ItemOrderIngredientController!{% endblock %}

{% block body %}

    <table id="item_order_ingredient" class="table table-striped table-field">
        <thead>
        <tr>
            <th >id</th>
            <th >item_order_id</th>
            <th >ingredient_id</th>
        </tr>
        </thead>
        <tbody>
        {% for itemOrderIngredient in itemOrderIngredients %}

            <tr>
                <td >{{ itemOrderIngredient.id }}</td>
                <td >{{ itemOrderIngredient.itemOrder.id }}</td>
                <td >{{ itemOrderIngredient.ingredient.id }}</td>

            </tr>
        {% endfor %}

        </tbody>
    </table>
{% endblock %}
